{
  "name": "restaurant-reviews",
  "version": "1.0.0",
  "description": "Get reviews of local restaurants near you",
  "main": "index.js",
  "scripts": {
    "debug": "node inspect app.js",
    "server": "node app.js",
    "start": "python3 -m http.server 8000",
    "prod-start": "cd dist && python3 -m http.server 8000",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run server\"",
    "prod": "concurrently --kill-others \"npm run prod-start\" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CDharmateja/mws-restaurant-stage-1.git"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-responsive": "^2.11.0",
    "gulp-uglify": "^3.0.1",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "include-all": "^1.0.0",
    "rc": "^1.2.8",
    "sails": "^0.12.13",
    "sails-disk": "~0.10.9"
  },
  "author": "C Dharmateja",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CDharmateja/mws-restaurant-stage-1/issues"
  },
  "homepage": "https://github.com/CDharmateja/mws-restaurant-stage-1#readme"
}
